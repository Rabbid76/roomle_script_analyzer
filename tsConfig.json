{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "outDir": "built"
    },
    "paths": {
        "@/*": [
          "src/*"
        ]
      },
    "include": [
        "src/**/*.ts"
      ],
      "exclude": [
        "node_modules"
      ]
}